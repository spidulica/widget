<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginStart="10dp"
    android:layout_marginEnd="10dp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/interval"
        android:text="interval"
        android:gravity="center"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/profesor"
        android:layout_marginEnd="10dp"/>
    <TextView
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/materie"
        android:text="materie"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/interval"
        android:layout_alignParentEnd="true"/>
    <TextView
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/locatie"
        android:text="locatie"
        android:layout_below="@+id/materie"
        android:layout_toEndOf="@+id/interval"
        android:layout_alignParentEnd="true"/>
    <TextView
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/profesor"
        android:text="profesor"
        android:layout_below="@+id/locatie"
        android:layout_alignStart="@+id/locatie"
        android:layout_alignParentEnd="true"/>

</RelativeLayout>